#№1
num = float(input())


integer_part = int(num)


print(integer_part % 3 == 0)
#№2
X = float(input())


fraction = X - int(X)


print(fraction > 0.50)
#№3
a, b = map(int, input().split())

print(b != 0 and a % b == 0)
#№4
a, b, c = map(int, input().split())

print(a + b > c and a + c > b and b + c > a)
#№5
x = float(input())

print((0 <= x <= 2) or (10 <= x <= 20))